{
  "name": "biotech-js",
  "version": "1.0.0",
  "description": "Package developed at A&A Biotechnology for reading all kinds of biotechnology related files",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "start:watch": "nodemon --watch \"dist/**\" dist/app.js",
    "build": "tsc src/app.ts --outDir dist",
    "build:watch": "tsc src/app.ts --watch --outDir dist",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint --fix --ext .js,.ts src",
    "lint:typecheck": "tsc src/app.ts --noEmit --outDir dist"
  },
  "author": "Tomasz DÄ…browski",
  "license": "MIT",
  "engines": {
    "node": ">=18.10.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/rollup-plugin-node-builtins": "^2.1.2",
    "@types/rollup-plugin-node-globals": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "biotechnology",
    "biotech",
    "genbank",
    "fasta",
    "fastq",
    "gbk",
    "gp",
    "proteinase",
    "sequence"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:tomek7667/biotech-js.git"
  },
  "files": [
    "dist/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
